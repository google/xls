// args: bits[7]:0x1; bits[7]:0x3f
fn main(x8607: u7, x8608: u7) -> u7 {
    let x8610: u7 = (x8607) & (x8608);
    let x8611: u7 = one_hot_sel(x8608 as u2, [x8610, x8608]);
    x8611
}


