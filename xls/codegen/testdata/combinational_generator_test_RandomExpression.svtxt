module RandomExpression(
  input wire [7:0] a,
  input wire [7:0] b,
  input wire [7:0] c,
  output wire [7:0] out
);
  // lint_off MULTIPLY
  function automatic [7:0] umul8b_8b_x_8b (input reg [7:0] lhs, input reg [7:0] rhs);
    begin
      umul8b_8b_x_8b = lhs * rhs;
    end
  endfunction
  // lint_on MULTIPLY
  wire [7:0] diff;
  wire [7:0] umul_12;
  wire [7:0] umul_13;
  wire [7:0] the_output;
  assign diff = a - b;
  assign umul_12 = umul8b_8b_x_8b(diff, diff);
  assign umul_13 = umul8b_8b_x_8b(c, diff);
  assign the_output = umul_12 + umul_13;
  assign out = the_output;
endmodule
