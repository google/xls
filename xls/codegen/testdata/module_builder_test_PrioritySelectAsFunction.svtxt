module PrioritySelectAsFunction(
  input wire [2:0] s,
  input wire [31:0] x,
  input wire [31:0] y,
  input wire [31:0] z,
  input wire [31:0] d
);
  function automatic [31:0] priority_sel_32b_3way (input logic [2:0] sel, input logic [31:0] case0, input logic [31:0] case1, input logic [31:0] case2, input logic [31:0] default_value);
    begin
      unique casez (sel)
        3'b??1: begin
          priority_sel_32b_3way = case0;
        end
        3'b?10: begin
          priority_sel_32b_3way = case1;
        end
        3'b100: begin
          priority_sel_32b_3way = case2;
        end
        3'b000: begin
          priority_sel_32b_3way = default_value;
        end
        default: begin
          // Propagate X
          priority_sel_32b_3way = 'X;
        end
      endcase
    end
  endfunction
  wire [31:0] priority_select;
  assign priority_select = priority_sel_32b_3way(s, x, y, z, d);
endmodule
