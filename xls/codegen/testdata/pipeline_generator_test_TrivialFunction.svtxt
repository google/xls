module TrivialFunction(
  input wire clk,
  input wire [7:0] x,
  output wire [7:0] out
);
  // ===== Pipe stage 0:

  // Registers for pipe stage 0:
  reg [7:0] p0_x;
  always_ff @ (posedge clk) begin
    p0_x <= x;
  end

  // ===== Pipe stage 1:

  // Registers for pipe stage 1:
  reg [7:0] p1_x;
  always_ff @ (posedge clk) begin
    p1_x <= p0_x;
  end
  assign out = p1_x;
endmodule
