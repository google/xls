module TwoBitSelector(
  input wire clk,
  input wire [1:0] s,
  input wire [7:0] x,
  input wire [7:0] y,
  input wire [7:0] z,
  output wire [7:0] out
);
  // ===== Pipe stage 0:

  // Registers for pipe stage 0:
  logic [1:0] p0_s;
  logic [7:0] p0_x;
  logic [7:0] p0_y;
  logic [7:0] p0_z;
  always_ff @ (posedge clk) begin
    p0_s <= s;
    p0_x <= x;
    p0_y <= y;
    p0_z <= z;
  end

  // ===== Pipe stage 1:
  wire [7:0] p1_sel_18_comb;
  assign p1_sel_18_comb = p0_s == 2'h0 ? p0_x : (p0_s == 2'h1 ? p0_y : p0_z);

  // Registers for pipe stage 1:
  logic [7:0] p1_sel_18;
  always_ff @ (posedge clk) begin
    p1_sel_18 <= p1_sel_18_comb;
  end
  assign out = p1_sel_18;
endmodule
