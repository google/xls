module AddNegateFlopNeitherInputsNorOutputs(
  input wire clk,
  input wire [7:0] x,
  input wire [7:0] y,
  output wire [7:0] out
);
  // ===== Pipe stage 0:
  wire [7:0] p0_add_7_comb;
  assign p0_add_7_comb = x + y;

  // Registers for pipe stage 0:
  logic [7:0] p0_add_7;
  always_ff @ (posedge clk) begin
    p0_add_7 <= p0_add_7_comb;
  end

  // ===== Pipe stage 1:
  assign out = -p0_add_7;
endmodule
