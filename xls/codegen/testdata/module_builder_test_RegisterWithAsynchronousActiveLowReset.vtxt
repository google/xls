module RegisterWithAsynchronousActiveLowReset(
  input wire [31:0] x,
  input wire [31:0] y,
  input wire clk,
  input wire rst,
  output wire [31:0] out
);
  reg [31:0] a;
  reg [31:0] b;
  always @ (posedge clk or negedge rst) begin
    if (!rst) begin
      a <= 32'h0000_0000;
      b <= 32'h0000_0042;
    end else begin
      a <= x + y;
      b <= y;
    end
  end
  assign out = a + b;
endmodule
