module main(
  input wire p,
  input wire [15:0] x,
  input wire [15:0] y,
  input wire [15:0] d,
  output wire [15:0] out
);
  wire [1:0] concat_27;
  wire [2:0] one_hot_18;
  assign concat_27 = {~p, p};
  assign one_hot_18 = {concat_27[1:0] == 2'h0, concat_27[1] && !concat_27[0], concat_27[0]};
  assign out = x & {16{concat_27[0]}} | y & {16{concat_27[1]}};
endmodule
