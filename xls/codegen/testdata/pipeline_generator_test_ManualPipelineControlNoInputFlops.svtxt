module ManualPipelineControlNoInputFlops(
  input wire clk,
  input wire [3:0] load_enable,
  input wire [1:0] s,
  input wire [7:0] x,
  input wire [7:0] y,
  input wire [7:0] z,
  input wire [7:0] a,
  output wire [7:0] out
);
  // ===== Pipe stage 0:

  // Registers for pipe stage 0:
  logic [1:0] p0_s;
  logic [7:0] p0_x;
  logic [7:0] p0_y;
  logic [7:0] p0_z;
  logic [7:0] p0_a;
  always_ff @ (posedge clk) begin
    p0_s <= load_enable[0] ? s : p0_s;
    p0_x <= load_enable[0] ? x : p0_x;
    p0_y <= load_enable[0] ? y : p0_y;
    p0_z <= load_enable[0] ? z : p0_z;
    p0_a <= load_enable[0] ? a : p0_a;
  end

  // ===== Pipe stage 1:

  // Registers for pipe stage 1:
  logic [1:0] p1_s;
  logic [7:0] p1_x;
  logic [7:0] p1_y;
  logic [7:0] p1_z;
  logic [7:0] p1_a;
  always_ff @ (posedge clk) begin
    p1_s <= load_enable[1] ? p0_s : p1_s;
    p1_x <= load_enable[1] ? p0_x : p1_x;
    p1_y <= load_enable[1] ? p0_y : p1_y;
    p1_z <= load_enable[1] ? p0_z : p1_z;
    p1_a <= load_enable[1] ? p0_a : p1_a;
  end

  // ===== Pipe stage 2:

  // Registers for pipe stage 2:
  logic [1:0] p2_s;
  logic [7:0] p2_x;
  logic [7:0] p2_y;
  logic [7:0] p2_z;
  logic [7:0] p2_a;
  always_ff @ (posedge clk) begin
    p2_s <= load_enable[2] ? p1_s : p2_s;
    p2_x <= load_enable[2] ? p1_x : p2_x;
    p2_y <= load_enable[2] ? p1_y : p2_y;
    p2_z <= load_enable[2] ? p1_z : p2_z;
    p2_a <= load_enable[2] ? p1_a : p2_a;
  end

  // ===== Pipe stage 3:
  wire [7:0] p3_sel_6_comb;
  assign p3_sel_6_comb = p2_s == 2'h0 ? p2_x : (p2_s == 2'h1 ? p2_y : (p2_s == 2'h2 ? p2_z : p2_a));

  // Registers for pipe stage 3:
  logic [7:0] p3_sel_6;
  always_ff @ (posedge clk) begin
    p3_sel_6 <= load_enable[3] ? p3_sel_6_comb : p3_sel_6;
  end
  assign out = p3_sel_6;
endmodule
