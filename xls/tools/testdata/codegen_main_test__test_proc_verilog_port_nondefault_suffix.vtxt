module neg_proc(
  input wire [31:0] in_d,
  input wire in_v,
  input wire out_r,
  output wire in_r,
  output wire [31:0] out_d,
  output wire out_v
);
  wire stage_outputs_valid_0;
  wire [31:0] negate;
  assign stage_outputs_valid_0 = in_v & out_r;
  assign negate = -in_d;
  assign in_r = stage_outputs_valid_0;
  assign out_d = negate;
  assign out_v = in_v;
endmodule
