module neg_proc(
  input wire [31:0] in,
  input wire in_vld,
  input wire out_rdy,
  output wire in_rdy,
  output wire [31:0] out,
  output wire out_vld
);
  wire stage_outputs_valid_0;
  wire [31:0] negate;
  assign stage_outputs_valid_0 = in_vld & out_rdy;
  assign negate = -in;
  assign in_rdy = stage_outputs_valid_0;
  assign out = negate;
  assign out_vld = in_vld;
endmodule
