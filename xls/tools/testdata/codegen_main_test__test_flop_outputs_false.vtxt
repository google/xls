module not_add(
  input wire clk,
  input wire [31:0] x,
  input wire [31:0] y,
  output wire [31:0] out
);
  // ===== Pipe stage 0:

  // Registers for pipe stage 0:
  reg [31:0] x__input_flop;
  reg [31:0] y__input_flop;
  always_ff @ (posedge clk) begin
    x__input_flop <= x;
    y__input_flop <= y;
  end

  // ===== Pipe stage 1:
  wire [31:0] p1_add_20_comb;
  assign p1_add_20_comb = x__input_flop + y__input_flop;
  assign out = ~p1_add_20_comb;
endmodule
